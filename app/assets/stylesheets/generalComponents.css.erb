/* Buttons - ( width must be defined in especific stylesheet; span's width 10px larger than it's parent.  )*/
.smallButton { background: transparent url(<%= asset_path 'small-button.png' %> no-repeat; display: block; height: 40px; }
.smallButton span{ background: transparent url(<%= asset_path 'small-button.png' %> no-repeat 56px -42px; display: block; color: #FFFFFF; display: block; font-weight: bold; line-height: 31px; text-align: center; text-shadow: 1px 1px 1px #006600; height: 40px; }
