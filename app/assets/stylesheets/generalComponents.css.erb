/* Lateral Menu */
	.leftSidemenu { float: left; width: 110px; margin-top: 55px; overflow: hidden; }
	.leftSidemenu li { list-style: none; }
	
		.leftSidemenu li:first-child a { background: url(<%= asset_path 'menu.png' %> no-repeat 0 0; }
		.leftSidemenu li a { background: url(<%= asset_path 'menu.png' %> no-repeat 0 -35px; color: #333; display: block; font: normal normal bold 11px/12px Verdana, Sans-Serif; height: 22px; padding: 6px 6px 7px; text-shadow: -1px 1px 0 #FFF; }
			.leftSidemenu li a span { color: #999; display: block; font: normal normal normal 10px/11px Verdana, Sans-Serif; }
		.leftSidemenu li:last-child a { background: url(<%= asset_path 'menu.png' %> no-repeat 0 -70px; }
	
		.leftSidemenu li:first-child a:hover,
		.leftSidemenu li:first-child a:focus,
		.leftSidemenu li:first-child a.ativo { background: url(<%= asset_path 'menu.png' %> no-repeat 0 -105px; text-decoration: none; }
