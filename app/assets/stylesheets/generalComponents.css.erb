/*Tickets*/
	
	/* styles for ALL tickets */
	.discountTicket { z-index: 1; display: block; position: absolute; opacity: 1 !important; }
	.discountTicket.normal { background: transparent url(<%= asset_path 'tickets_normal.png' %>) no-repeat; }
	.discountTicket.normal *{ color: #516117; }
	.discountTicket.special { background: transparent url(<%= asset_path 'tickets_gold.png' %>) no-repeat; }
	.discountTicket.special *{ color: #7B5800; }
	.discountTicket.horizontal { background: transparent url(<%= asset_path 'ticket_horizontal.png' %>) no-repeat; }
	.discountTicket.horizontal *{ color: #516117; }
	.discountTicket strong{ display: block; letter-spacing: -3px; text-align: center; }
	.discountTicket strong sup { font-size: 10px; }
	
	/* styles for HORIZONTAL tickets */
	.discountTicket.horizontal { float: right; right: -17px; text-align: center; width: 191px; height: 66px; }
		.discountTicket.horizontal span { text-indent: -9999px; display: block; }
		.discountTicket.horizontal strong { margin: 0 0 0 40px; width: 135px; clear: both; font: bold 30px/48px Arial, Helvetica, Sans-Serif !important; }
		.discountTicket.horizontal strong span { text-indent: 0; display: inline; font: bold 11px/11px Arial,Helvetica,Sans-Serif !important; margin: 0 5px; }
	
	/* styles for BIG tickets */
	.bigTicket {  height: 200px; right: -23px; top: 5px; width: 90px; }
	.bigTicket.normal { background-position: -23px 0; }
	.bigTicket.special { background-position: -24px 0; }
		.bigTicket strong { font: bold 35px/35px Arial, Helvetica, Sans-Serif; padding: 115px 0 4px 6px;  }
	
	/* styles for SMALL tickets */
	.smallTicket { height: 120px; right: -15px; top: 5px; width: 50px; }
	.smallTicket.normal { background-position: -310px -74px; }
	.smallTicket.special { background-position: -309px -74px; }
		.smallTicket strong { font: bold 28px/35px Arial, Helvetica, Sans-Serif; padding: 60px 0 4px 0px; width: 42px; }
	
	/* styles for MEDIUM tickets */
	.mediumTicket { height: 140px; right: -15px; top: 5px; width: 57px; }
	.mediumTicket.normal { background-position: -175px -51px; }
	.mediumTicket.special { background-position: -175px -50px; }
		.mediumTicket strong { font: bold 28px/35px Arial, Helvetica, Sans-Serif; padding: 77px 0 4px 0px; width: 52px; }
	
	/* related styles */
	div#pagina div#corpo div.ofertas div.oferta div.autor.special { background: #ffe89b; height: auto !important; padding: 15px 45px 15px 115px !important; }
	div#pagina div#corpo div.ofertas div.oferta div.autor.special a { color: #996600; }
	div.paginaOferta div.detalhes div.desconto div.comprar.afterTicket { margin-top: 35px; }
  div.ofertas div.semOferta strong.faixa.ticket { background: url(<%= asset_path 'faixa-sem-oferta.jpg' %>) no-repeat -45px 0; color: #fff; display: block; font: 15px/45px Arial, Helvetica, Sans-Serif; height: 50px; margin: 0 0 10px -25px; position: relative; text-indent: 10px; width: 250px; }
	
	/* specific changes */
		
		/* my panel */
		.smallTicket.mypanel.special { right: -14px; }
		.smallTicket.mypanel.normal { right: -14px; }
		
		/* ticket withdraw */
		.discountTicket.horizontal.ticketWithdraw { right: -11px; }
		
		/* benefit */
		.noTicketDiscount { margin-top: 27px; }

		
