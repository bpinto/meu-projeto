		.leftSidemenu li:last-child a:focus,
		.leftSidemenu li:last-child a.ativo { background: url(<%= asset_path 'menu.png' %> no-repeat 0 -175px;  text-decoration: none; }
	
	/* Big menu */
	.leftSidemenu.big { width: 180px; }
		.leftSidemenu.big li:first-child a { background: url(<%= asset_path 'menu.png' %> no-repeat 0 -210px; }
		.leftSidemenu.big li a { background: url(<%= asset_path 'menu.png' %> no-repeat 0 -245px; }
		.leftSidemenu.big li:last-child a { background: url(<%= asset_path 'menu.png' %> no-repeat 0 -280px; }
