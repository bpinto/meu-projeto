div.side div.default_window div.title { background: #EEE; border-bottom: 1px solid #CCC; padding: 15px 20px; -moz-border-radius: 10px 10px 0 0; -webkit-border-radius: 10px 10px 0 0; border-radius: 10px 10px 0 0; }

div.side div.default_window div.title h3 { font-size: 16px; }

div.side div.default_window div.content { width: 290px; border-top: 1px solid white; padding: 10px; overflow: hidden; }

div.side div.default_window div.content .userPicture { border: none; display: block; }

div.side div.default_window div.banner { float: left; display: block; }

div.side a:hover { text-decoration: none; }

div.buttonPanel { margin: 5px 0px 10px 0px;}

div.buttonPanel a.signupButton { background: url(<%= asset_path 'signup_button.png' %>) no-repeat; display: inline-block; width: 155px; height: 58px; text-indent: -999em;}

div.buttonPanel a.signinButton { background: url(<%= asset_path 'signin_button.png' %>) no-repeat; display: inline-block; width: 155px; height: 58px; text-indent: -999em;}

div.buttonPanel a.profileButton { background: url(<%= asset_path 'profile_button.png' %>) no-repeat; display: inline-block; width: 155px; height: 58px; text-indent: -999em;}

div.buttonPanel a.shareButton { background: url(<%= asset_path 'share_button.png' %>) no-repeat; display: inline-block; width: 155px; height: 58px; text-indent: -999em;}