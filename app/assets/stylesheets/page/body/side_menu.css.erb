div.side div.defaultWindow div.title { background: #EEE; border-bottom: 1px solid #CCC; padding: 15px 20px; -moz-border-radius: 10px 10px 0 0; -webkit-border-radius: 10px 10px 0 0; border-radius: 10px 10px 0 0; }

div.side div.defaultWindow div.title h3 { font-size: 16px; }

div.side div.defaultWindow div.content { width: 290px; border-top: 1px solid white; padding: 10px; overflow: hidden; }

div.side div.defaultWindow div.content .userPicture { border: none; display: block; }

div.side div.defaultWindow div.banner { float: left; display: block; }