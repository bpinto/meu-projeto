					div.outrasAcoes div.compartilhar strong { color: #4C92AC; display: block; font: italic bold 16px/16px Arial, Helvetica, Sans-Serif; margin-bottom: 15px; }
					
					div.outrasAcoes div.compartilhar div.formularioPadrao { margin-bottom: 15px; overflow: hidden; }
						div.outrasAcoes div.compartilhar div.formularioPadrao label { float: left; font: bold 12px/26px Verdana, Sans-Serif; height: 26px; /* tirar */ background: none !important; width: auto !important; padding: 0 !important; }
						div.outrasAcoes div.compartilhar div.formularioPadrao span { display: block; margin-left: 45px; }
							div.outrasAcoes div.compartilhar div.formularioPadrao span input { width: 95%; }
					
					div.outrasAcoes div.compartilhar a.twitter { background: #eee url(<%= asset_path 'twitter.png' %>) no-repeat 5px center; border: 1px solid #ccc; color: #999; display: block; float: left; font: bold 11px/24px Verdana, Sans-Serif; height: 25px; margin-bottom: 15px; padding: 0 8px 0 35px; /**/ border-radius: 3px; -moz-border-radius: 3px; -webkit-border-radius:  3px; }
					div.outrasAcoes div.compartilhar a.twitter:hover { border-color: #4C92AC; color: #4C92AC; }
					
					div.outrasAcoes div.compartilhar p { clear: both; margin-bottom: 15px; }
					div.outrasAcoes div.compartilhar p,
						div.outrasAcoes div.compartilhar p a { font: 11px/13px Verdana, Sans-Serif; }
					div.outrasAcoes div.compartilhar p.conectarConta { font-size: 11px; line-height: 16px; }
					
					div.outrasAcoes div.compartilhar a.conectar { background: #C6E479 url(<%= asset_path 'conectar-conta-social.jpg' %>) no-repeat 3px center; float: left; margin-bottom: 15px; }
						div.outrasAcoes div.compartilhar a.conectar span { padding-left: 40px; }
					div.outrasAcoes div.compartilhar a.cancelar { clear: both; color: #c00; display: block; float: left; font: 10px/10px Verdana, Sans-Serif; text-decoration: underline; }
					div.outrasAcoes div.compartilhar a.cancelar:hover { text-decoration: none; }
				
				/* denunciar */
				div.outrasAcoes div.denunciar { background: #fff; border: 1px dashed #ddd; margin-bottom: 15px; overflow: hidden; padding: 15px; }
					
					div.outrasAcoes div.denunciar strong { color: #c00; display: block; font: italic bold 16px/16px Arial, Helvetica, Sans-Serif; margin-bottom: 15px; }
					
					div.outrasAcoes div.denunciar p { margin: 0 0 15px !important; }
					div.outrasAcoes div.denunciar p,
						div.outrasAcoes div.denunciar p a { font: 11px/13px Verdana, Sans-Serif; }
						
						div.outrasAcoes div.denunciar ul { display: block; overflow: hidden; *padding-bottom: 15px; }
							div.outrasAcoes div.denunciar ul li { display: block; float: left; margin-bottom: 15px; overflow: hidden; width: 180px; }
								div.outrasAcoes div.denunciar ul li span { }
									div.outrasAcoes div.denunciar ul li span input { float: left; height: 14px; margin-right: 5px; width: 14px; }
									div.outrasAcoes div.denunciar ul li span label { float: left; }
						
						div.outrasAcoes div.denunciar div.formularioPadrao { padding-bottom: 15px; }
							div.outrasAcoes div.denunciar div.formularioPadrao textarea { width: 96%; }
									
						div.outrasAcoes div.denunciar a.botaoPadrao { float: left; clear: both; }						
						div.outrasAcoes div.denunciar a.cancelar { color: #c00; float: left; font: 10px/25px Verdana, Sans-Serif; margin: 0 0 0 15px !important; text-decoration: underline; }
						
				/* excluir */
				div.outrasAcoes div.excluir { }
				
					div.outrasAcoes div.excluir ul {}
						div.outrasAcoes div.excluir ul li { float: none; width: auto; }
	
	/* topico especial */
	div.topicoEspecial { padding-top: 20px; }
	
		div.topicoEspecial div.comentario { background: #EAEDE0; border-color: #BDC7A1; position: relative; *min-height: 127px; }
	
			div.topicoEspecial div.comentario .especial { color: #798C3E; display: block; font: 10px/20px Arial, Helvetica, Sans-Serif; height: 20px; left: 15px; position: absolute; text-decoration: none; top: -20px; }
				div.topicoEspecial div.comentario a.especial:hover { text-decoration: underline; }
					div.topicoEspecial div.comentario a.especial strong { background: #798C3E; color: #fff; display: inline-block; font: bold 10px/20px Arial, Helvetica, Sans-Serif; height: 20px; margin-right: 2px; padding: 0 7px; text-transform: uppercase; /**/ -moz-border-radius: 3px 3px 0 0; -webkit-border-radius: 3px 3px 0 0; }
					
			div.topicoEspecial .foto,
			div.topicoEspecial a.foto:hover { border-color: #9FB762 !important; }
				
