		div.topicoEstabelecimentoPqn .estabelecimentoPadrao { height: 30px; width: 40px; }
		
		/* comentario */
		div.topicoEstabelecimentoGde div.comentario { margin-left: 108px; }
		div.topicoEstabelecimentoPqn div.comentario { margin-left: 63px; }
		
