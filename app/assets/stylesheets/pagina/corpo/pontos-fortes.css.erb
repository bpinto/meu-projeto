									div.pontosFortes div.topicoPadrao div.conteudo div.acoesDica ul li.ativo ul li:first-child a { border-top: 0; /**/ -moz-border-radius: 5px 5px 0 0; -webkit-border-radius: 5px 5px 0 0; }
