/* BAIRRO */
div.estabelecimentosBairro { background: #fff; }
	div.estabelecimentosBairro .barra h3 { font-size: 16px; }
	div.estabelecimentosBairro div.conteudo { width: 290px; border-top: 1px solid #fff; padding: 10px; /**/ overflow: hidden; }

	div.estabelecimentosBairro ul { float: left; margin: 3px 10px; }
		div.estabelecimentosBairro ul li { border-radius: 10px; -moz-border-radius: 10px; -webkit-border-radius: 10px; background: #eee; color: #666; margin: 5px; display: block; float: left; }
			div.estabelecimentosBairro ul li a { white-space: nowrap; height: 25px; display: block; border-radius: 10px; -moz-border-radius: 10px; -webkit-border-radius: 10px; color: #666; padding: 3px 8px; font: 12px/25px Arial; text-decoration: none; text-transform: lowercase; }
			div.estabelecimentosBairro ul li a:hover { background: #859944; color: #fff; }
				div.estabelecimentosBairro ul li span { color: #4C92AC; font-weight: bold; }
