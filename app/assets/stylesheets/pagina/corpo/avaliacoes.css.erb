				div.avaliacoes div.topico div.comentario div.acoesAvaliacao ul li.outrasAcoes { position: relative; }
					div.avaliacoes div.topico div.comentario div.acoesAvaliacao ul li.outrasAcoes a { background: url(<%= asset_path 'seta.png' %>) no-repeat right 10px; padding-right: 15px; }
