	div.avaliacoes div.outrasAcoes { clear: both; display: block; overflow: hidden; /* tirar */ margin: 0 !important; }
		div.avaliacoes div.outrasAcoes div.compartilhar,
		div.avaliacoes div.outrasAcoes div.denunciar,
