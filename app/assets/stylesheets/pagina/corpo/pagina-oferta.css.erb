				div.paginaOferta .confirmarDados ol li input.certo { background: #F1F5E5 url(<%= asset_path 'icones.png' %>) no-repeat right -35px; border-color: #789F00 #C9D999 #C9D999 #789F00; color: #789F00 !important; padding-right: 30px; }
				div.paginaOferta .confirmarDados ol li input.errado { background: #FAE5E5 url(<%= asset_path 'icones.png' %>) no-repeat right -305px; border-color: #C00 #EB9999 #EB9999 #C00; color: #C00 !important; padding-right: 30px; }		
				
