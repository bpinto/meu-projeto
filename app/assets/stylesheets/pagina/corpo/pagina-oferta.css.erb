				div.paginaOferta .confirmarDados ol li select { min-width: 170px; }
				div.paginaOferta .confirmarDados ol li span { display: block; float: left; font: italic 10px/22px Arial, Helvetica, Sans-Serif; margin-right: 15px; }
				div.paginaOferta .confirmarDados ol li span.descricao { background: url(<%= asset_path 'icones.png' %>) no-repeat -8px -1175px; color: #4C92AC; font: 10px/20px Verdana, Sans-Serif; padding-left: 20px; text-decoration: underline; }
				div.paginaOferta .confirmarDados ol li strong { color: #569F0D; display: block; font: normal bold 13px/22px Arial, Helvetica, Sans-Serif;  margin: 0 0 2px 0; width: 525px; }
				div.paginaOferta .confirmarDados ol li a { display: block; float: left; font: 11px/11px Verdana, Sans-Serif; padding: 6px 0; }
