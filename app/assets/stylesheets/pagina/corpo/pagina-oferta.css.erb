div.principal { *padding-bottom: 15px; }

/* pagina oferta */
div.paginaOferta { padding: 15px 15px 0; position: relative; }
	
	/* altura 100% */
	div.altura { display: block; *display: table; position: relative; margin-bottom: 15px; width: 975px; }
	div.altura:after { content: "."; display: block; height: 0; clear: both; visibility: hidden; }
		
		div.altura div.lateral { min-height: 460px; }
		div.altura div.lateral .mapTitle h3{ color: #547100; font: italic bold 18px/18px Georgia; text-shadow: 1px 1px 0 #FFFFFF; }
		div.altura div.paginaOferta { margin-bottom: 0; }
		div.altura div.localizacao { position: relative; width: 315px; }
		div.altura div.lateral .backOffer { color: #cc0000; float: right; text-decoration: underline; margin: 5px 10px 10px 0; }
		div.altura div.lateral .backOffer:hover,
		div.altura div.lateral .backOffer:focus { text-decoration: none; }                   
		.altura .securityAdvice { position: relative; width: 275px; padding: 30px 20px; background: #ffffff; }
			.altura .securityAdvice .imgThawte { height: 200px; float: left; background: transparent url(<%= asset_path 'security_badge.jpg' %>) no-repeat; padding: 0 15px 0 0; }
			.altura .securityAdvice h3 { color: #547100; font-size: 16px; margin: 0 0 10px; }
			.altura .securityAdvice p { font: normal 12px/18px Arial; }
		
		div.altura div.lateral div.oferta { border: none; /**/ box-shadow: none; -webkit-box-shadow: none; -moz-box-shadow: none; }  
	
	/* titulo */
	div.paginaOferta div.titulo { display: block; padding: 0 15px 10px; }
	.paginaOferta .titulo.paymentContentTitle { border: 1px solid #e5e5e5; padding: 5px; margin: 10px 15px; border-radius: 10px; -moz-border-radius: 10px; -webkit-border-radius: 10px; }
	.paginaOferta .titulo.paymentContentTitle { zoom: 1; display: block; }
	.paginaOferta .paymentContentTitle:after { content: "."; display: block; height: 0; clear: both; visibility: hidden;   }
		div.paginaOferta div.titulo h1 { color: #4C92AC; display: block; font-size: 22px; margin-bottom: 15px; }
		div.paginaOferta div.titulo .paymentTitle { float: left; padding: 0 0 0 10px; width: 455px; margin: 0; }
		div.paginaOferta div.titulo .imgAdvertisementPayment { float: left; padding: 5px; border: 1px solid #d0d0d0; }
		div.paginaOferta div.titulo h2 { color: #333; display: block; font: bold 20px/20px Arial, Helvetica, Sans-Serif; margin-bottom: 5px; }
		div.paginaOferta div.titulo .backSite { display: block; float:left; width: 20px; height: 20px; text-decoration: none; border: none; outline: none; background: transparent url(<%= asset_path 'seta-back-estabelecimento.png' %>) no-repeat ; margin: 5px 5px 0 0; text-indent: -9999px;}
		div.paginaOferta div.titulo .backSite:hover,
		div.paginaOferta div.titulo .backSite:focus { background-position: 0 -22px ; } 
	
	/* detalhes */
	div.paginaOferta div.detalhes { display: block; height: 232px; padding: 0 15px 30px; }
		
		/* foto */
		div.paginaOferta div.detalhes div.foto { background: #fff; border: 1px solid #ccc; display: block; float: left; margin-right: 30px; overflow: hidden; padding: 5px; /**/ box-shadow: 1px 1px 3px #ddd; -webkit-box-shadow: 1px 1px 3px #ddd; -moz-box-shadow: 1px 1px 3px #ddd; }
			div.paginaOferta div.detalhes div.foto div { height: 220px; overflow: hidden; width: 300px; }
		
		/* desconto */	
		div.paginaOferta div.detalhes div.desconto { display: block; float: left; margin-bottom: 15px; padding-top: 10px; width: 240px; }	
		div.paginaOferta div.detalhes div.semOferta { padding-top: 70px; position: relative; }
		div.paginaOferta div.detalhes div.status { padding-top: 40px; }
			
			div.paginaOferta div.detalhes div.desconto div.porcentagem { background: url(<%= asset_path 'bg-oferta-porcentagem.png' %>) no-repeat; display: block; height: 200px; position: absolute; right: -23px; top: 5px; width: 90px; }
					div.paginaOferta div.detalhes div.desconto div.porcentagem strong { color: #516117; display: block; font: bold 35px/35px Arial, Helvetica, Sans-Serif; letter-spacing: -3px; padding: 109px 0 4px 6px; text-align: center; }
						div.paginaOferta div.detalhes div.desconto div.porcentagem strong sup { color: #516117; font-size: 10px; }
					div.paginaOferta div.detalhes div.desconto div.porcentagem span { color: #516117; display: block; font: bold 15px/15px Arial, Helvetica, Sans-Serif; padding: 8px; text-align: center; text-transform: uppercase; }
				
				div.paginaOferta div.detalhes div.desconto div.de { display: block; margin-bottom: 5px; }
					div.paginaOferta div.detalhes div.desconto div.de span { color: #888; font: bold 15px/15px Arial, Helvetica, Sans-Serif; }
					div.paginaOferta div.detalhes div.desconto div.de strong { background: url(<%= asset_path 'bg-pagina-oferta-preco.png' %>) no-repeat center center; color: #900; font: bold 35px/35px Arial, Helvetica, Sans-Serif; }
						div.paginaOferta div.detalhes div.desconto div.de strong span { color: #900 !important; font: bold 20px/20px Arial, Helvetica, Sans-Serif; }
						
				div.paginaOferta div.detalhes div.desconto div.por { display: block; margin-bottom: 25px; }
					div.paginaOferta div.detalhes div.desconto div.por span { color: #888; font: bold 15px/15px Arial, Helvetica, Sans-Serif; }
					div.paginaOferta div.detalhes div.desconto div.por strong { color: #516017; font: bold 35px/35px Arial, Helvetica, Sans-Serif; letter-spacing: -2px; }
						div.paginaOferta div.detalhes div.desconto div.por strong span { color: #516017 !important; font: bold 20px/20px Arial, Helvetica, Sans-Serif; letter-spacing: 0; }
						
				div.paginaOferta div.detalhes div.desconto div.comprar { display: block; margin-bottom: 25px; overflow: hidden; }
					div.paginaOferta div.detalhes div.desconto div.comprar a.botao { background: url(<%= asset_path 'fundos-padrao.jpg' %>) repeat-x 0 -190px; border: 1px solid #834512; color: #834512; display: block; float: left; font: bold 17px/17px Arial, Helvetica, Sans-Serif; padding: 11px 10px 9px; text-align: center; text-decoration: none; text-shadow: 1px 1px 0 #F8D48A; /**/ -moz-box-shadow: inset 1px 1px 0 #FEEB99; -moz-border-radius: 5px; -webkit-border-radius: 5px; border-radius: 5px; }
					div.paginaOferta div.detalhes div.desconto div.comprar a.botao span { font: bold 17px/17px Arial, Helvetica, Sans-Serif; color: #834512; }
					div.paginaOferta div.detalhes div.desconto div.comprar a.botao:hover { background-position: 0 -230px; border-color: #C1611A; -moz-box-shadow: 1px 1px 3px #bbb; }
					div.paginaOferta div.detalhes div.desconto div.comprar a.botao span {  }
				div.paginaOferta div.detalhes div.desconto div.recomendacoes {}
					div.paginaOferta div.detalhes div.desconto div.recomendacoes span.legenda { color: #999; display: block; font: 11px/11px Verdana, Sans-Serif; margin-bottom: 10px; }
					div.paginaOferta div.detalhes div.desconto div.recomendacoes p { display: block; overflow: hidden; }
						div.paginaOferta div.detalhes div.desconto div.recomendacoes p em { color: #333; displat: block; float: left; font: bold 20px/30px Arial, Helvetica, Sans-Serif; letter-spacing: -1px; }
						div.paginaOferta div.detalhes div.desconto div.recomendacoes p em.icone { background: url(<%= asset_path 'icone-recomendacoes-pagina-oferta.jpg' %>) no-repeat 7px center; padding-left: 37px; }
			
				div.paginaOferta div.detalhes div.desconto strong.faixa { background: url(<%= asset_path 'faixa-sem-oferta.jpg' %>) no-repeat; color: #fff; display: block; font: 18px/45px Arial, Helvetica, Sans-Serif; height: 50px; left: -30px; position: absolute; text-indent: 10px; top: 0; width: 300px; }
				
				div.paginaOferta div.detalhes div.desconto div.aviso { display: block; }
					div.paginaOferta div.detalhes div.desconto div.aviso p { color: #666; display: block; font: bold 15px/17px Arial, Helvetica, Sans-Serif; margin-bottom: 25px; text-align: center; }
					div.paginaOferta div.detalhes div.desconto div.aviso a.botaoPadrao { margin: 0 auto 25px; width: 145px; }
					div.paginaOferta div.detalhes div.desconto div.aviso div.email { display: block; overflow: hidden; padding-bottom: 25px; }
						div.paginaOferta div.detalhes div.desconto div.aviso div.email input { border: 1px solid #aaa; color: #999; float: left; font-size: 11px; padding: 7px; width: 155px; /**/ border-radius: 3px; -webkit-border-radius: 3px; -moz-border-radius: 3px; }
						div.paginaOferta div.detalhes div.desconto div.aviso div.email input:focus { border-color: #4C92AC; color: #4C92AC; /**/ box-shadow: inset 1px 1px 5px #ccc; -webkit-box-shadow: inset 1px 1px 5px #ccc; -moz-box-shadow: inset 1px 1px 5px #ccc; }
						div.paginaOferta div.detalhes div.desconto div.aviso div.email a.botaoPadrao { float: right; margin: 0; width: auto; }
					div.paginaOferta div.detalhes div.desconto div.aviso a.saibaMais { background: url(<%= asset_path 'icones.png' %>) no-repeat -8px -1178px; color: #4C92AC; display: block; font: 10px/15px Verdana, Sans-Serif; padding-left: 20px; }
					
					div.paginaOferta div.detalhes div.feedback {}
						div.paginaOferta div.detalhes div.feedback p { color: #4C92AC; }
						div.paginaOferta div.detalhes div.feedback p.mensagem { background: #e5e5e5; color: #666; display: block; font: 11px/11px Arial, Helvetica, Sans-Serif !important; padding: 7px 10px; /**/ border-radius: 5px; -webkit-border-radius: 5px; -moz-border-radius: 5px; }
						
					div.paginaOferta div.detalhes div.status div.aviso {}
						div.paginaOferta div.detalhes div.status div.aviso p { color: #4C92AC; }
						div.paginaOferta div.detalhes div.status div.aviso p.mensagem { background: #ffc; border: 1px solid #fc0; color: #900; display: block; font: 15px/15px Arial, Helvetica, Sans-Serif; padding: 7px 10px; /**/ border-radius: 5px; -webkit-border-radius: 5px; -moz-border-radius: 5px; }
						
		/* deslogado */
		div.paginaOferta div.deslogado div.desconto div.de span { color: #ccc; }
		div.paginaOferta div.deslogado div.desconto div.de strong,
			div.paginaOferta div.deslogado div.desconto div.de strong span { color: #EB9999 !important; }
		div.paginaOferta div.deslogado div.desconto div.por span { color: #ccc; }
		div.paginaOferta div.deslogado div.desconto div.por strong,
			div.paginaOferta div.deslogado div.desconto div.por strong span { color: #B9BFA2 !important; }
		div.paginaOferta div.deslogado div.desconto div.recomendacoes span { color: #ccc !important; }
		div.paginaOferta div.deslogado div.desconto div.recomendacoes em { color: #aaa !important; }
		div.paginaOferta div.deslogado div.desconto div.porcentagem { background: url(<%= asset_path 'bg-oferta-cadeado.png' %>) no-repeat !important; height: 80px; opacity: 10; right: -37px !important; }
			div.paginaOferta div.deslogado div.desconto div.porcentagem * { text-indent: -999em; }
			
				div.paginaOferta div.deslogado div.desconto div.comprar { margin-bottom: 0; }
  				div.paginaOferta div.deslogado div.comprar a.botao { height: 40px; background: url(<%= asset_path 'bt_offer_available.png' %>) no-repeat scroll 0 0 transparent !important; padding:0 !important; border: none !important; width: 218px;text-shadow: none !important; width: 208px; -moz-border-radius: 0 !important; border-radius: 0 !important; -webkit-border-radius: 0 !important; box-shadow: none !important; -moz-box-shadow: none !important;  }
				div.paginaOferta div.deslogado div.comprar a.botao span{ width: 202px; background: url(<%= asset_path 'bt_offer_available.png' %>) no-repeat scroll 204px -42px transparent !important; padding: 0 10px 10px !important; display: block; color: #fff !important; font: bold 13px/30px Arial, Helvetica, Sans-Serif !important; }
				div.paginaOferta div.deslogado div.comprar a.botao:hover { background-position: 0 -80px !important; border: none !important; padding: 2px 0 0 0 !important; height: 38px; }
				div.paginaOferta div.deslogado div.comprar a.botao:hover span { background-position: 203px -125px !important; }
						
				div.paginaOferta div.deslogado .support_call { color: #666666; font: 10px/15px  Arial, Helvetica, Sans-Serif !important; margin: 0 0 10px 63px; }
	/* info */
	div.paginaOferta div.info { display: inline-block; *display: inline; padding: 0 10px 25px 15px; vertical-align: top; zoom: 1; }
	
		div.paginaOferta div.info div.topicoPadrao div.titulo img { margin-bottom: 15px !important; }
		
		div.paginaOferta div.info div.barra { background: #4C92AC; border: none; display: block; margin: 0; padding: 5px; /**/ border-radius: 5px 5px 0 0; -webkit-border-radius: 5px 5px 0 0; -moz-border-radius: 5px 5px 0 0; }
			div.paginaOferta div.info div.barra h2 { color: #fff; font: bold 14px/14px Arial, Helvetica, Sans-Serif; text-align: center; }
			
		div.paginaOferta div.info div.conteudo { border: 1px solid #ddd; border-top: none; display: block; padding: 20px 20px 5px; /**/ border-radius: 0 0 5px 5px; -webkit-border-radius: 0 0 5px 5px; -moz-border-radius: 0 0 5px 5px; }
			div.paginaOferta div.info div.conteudo ul.fixa { display: block; padding-bottom: 5px; width: 238px; }
				div.paginaOferta div.info div.conteudo ul.fixa li { background: url(<%= asset_path 'lista.png' %>) no-repeat -5px 0; color: #666; display: block; font: 11px/13px Verdana, Sans-Serif; list-style: none; padding-bottom: 10px; padding-left: 15px; }
				div.paginaOferta div.info div.conteudo ul.fixa li.validade { background: #ffc; color: #333; font: bold 14px/14px Arial, Helvetica, Sans-Serif; margin-bottom: 10px; padding: 5px; text-align: center; /**/ border-radius: 5px; -webkit-border-radius: 5px; -moz-border-radius: 5px; }
					div.paginaOferta div.info div.conteudo ul.fixa li.validade span { color: #c00; font: bold 14px/14px Arial, Helvetica, Sans-Serif; }
				
			div.paginaOferta div.info div.conteudo p { color: #666; display: block; font: 11px/15px Verdana, Sans-Serif; padding-bottom: 15px; width: 543px; }
			
			div.paginaOferta div.info div.conteudo div.dica { margin: 0 0 15px; width: 543px; }
				div.paginaOferta div.info div.conteudo div.dica div.titulo { padding: 0; }
					div.paginaOferta div.info div.conteudo div.dica div.titulo p { width: auto; }
						div.paginaOferta div.info div.conteudo div.dica div.titulo p a { text-decoration: none; }
						div.paginaOferta div.info div.conteudo div.dica div.titulo p a:hover { text-decoration: underline; }
					div.paginaOferta div.info div.conteudo div.dica div.titulo p.usuario { margin-bottom: 0; }
						div.paginaOferta div.info div.conteudo div.dica div.titulo p.usuario a { color: #859944; }
						
			div.paginaOferta div.info div.conteudo div.avaliacao { margin: 0 0 15px; width: 543px; }
				div.paginaOferta div.info div.conteudo div.avaliacao p.texto { padding: 0; width: auto; }
						
			div.paginaOferta div.foto { background: #fff; border: 1px solid #ccc; display: block; margin-bottom: 15px; padding: 5px; /**/ box-shadow: 1px 1px 3px #ddd; -webkit-box-shadow: 1px 1px 3px #ddd; -moz-box-shadow: 1px 1px 3px #ddd; }
				div.paginaOferta div.foto img { display: block; margin: auto; }
	
	/* formulario */
	div.paginaOferta .confirmarDados { padding: 0 15px 15px; }
		div.paginaOferta .stepList li { list-style: none; display: block; }
		div.paginaOferta .stepList li .indexStep { text-shadow: 1px 1px 0 #ce0b0b, -1px 1px 0 #ce0b0b, 1px -1px 0 #ce0b0b, -1px -1px 0 #ce0b0b; margin-right: 7px; -moz-border-radius: 5px 5px 5px 5px; border-radius: 5px 5px 5px 5px; -webkit-border-radius: 5px 5px 5px 5px; color: #FFFFFF; display: block; float: left; font: bold 23px/23px Gerogia; margin-right: 5px; padding: 5px 10px; background: #f55351; /* Old browsers */ background: -moz-linear-gradient(top, #f55351 0%, #c92523 100%); /* FF3.6+ */ background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f55351), color-stop(100%,#c92523)); /* Chrome,Safari4+ */ background: -webkit-linear-gradient(top, #f55351 0%,#c92523 100%); /* Chrome10+,Safari5.1+ */ background: -o-linear-gradient(top, #f55351 0%,#c92523 100%); /* Opera11.10+ */ background: -ms-linear-gradient(top, #f55351 0%,#c92523 100%); /* IE10+ */ filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f55351', endColorstr='#c92523',GradientType=0 ); /* IE6-9 */ background: linear-gradient(top, #f55351 0%,#c92523 100%); /* W3C */  }
			div.paginaOferta .confirmarDados ol { clear: both; background: #eee; padding: 10px 20px 10px 40px; /**/ border-radius: 5px 5px 5px 5px; -webkit-border-radius: 5px 5px 5px 5px; -moz-border-radius: 5px 5px 5px 5px; }
			div.paginaOferta .confirmarDados ol li { clear: both; display: block; list-style: none; position: relative; }
				
				div.paginaOferta .confirmarDados ol:after { content: "."; display: block; height: 0; clear: both; visibility: hidden; }
				
				div.paginaOferta .confirmarDados ol li:after { content: "."; display: block; height: 0; clear: both; visibility: hidden; }
				div.paginaOferta .confirmarDados ol li label { color: #333; display: block; font: bold 13px/22px Arial, Helvetica, Sans-Serif; }
				div.paginaOferta .confirmarDados ol li label.descLbl { font: normal 9px/12px Arial, Helvetica, Sans-Serif; }
				div.paginaOferta .confirmarDados ol li label.dateLbl { font: bold 10px/12px Arial, Helvetica, Sans-Serif; float: left; margin: 10px 10px 0 -7px; }
				div.paginaOferta .confirmarDados ol li label.dateLbl.sample { font: normal 10px/12px Arial, Helvetica, Sans-Serif; float: left; margin: 10px 10px 0 0px; }
