  /* CABECALHO */
  @import url(<%= asset_path "pagina/header.css" %>);

  /* PAGINA */
  div#page { background: #FFCC66;}

  /* GERAL */
  * { color: #666; font-size: 12px; font-family: Verdana, Sans-Serif; margin: 0; outline: none; padding: 0; }

  /* BODY */
  body { background: #FFCC66; }
  body#home { background: #FFCC66; }

  /* A */
  a { color: #003399; cursor: pointer; text-decoration: none; }
  a:hover { /* text-decoration: underline; */ }

  /* IMG */
  img { border: none; display: block; }
 
  /* H1 */
  h1 { color: #333; font: italic normal bold 30px/30px Arial, Helvetica, Sans-Serif; letter-spacing: -0.05em; text-shadow: 1px 1px 0 #FFF; }

  /* H2 */
  h2 { color: #333; font: italic normal bold 20px/20px Arial, Helvetica, Sans-Serif; }
   
  /* H3 */
  h3 { color: #333; font: italic normal bold 20px/20px Arial, Helvetica, Sans-Serif; text-shadow: 1px 1px 0 #FFF; }

  /* H4 */
  h4 { color: #333; font: italic normal bold 17px/17px Arial, Helvetica, Sans-Serif; text-shadow: 1px 1px 0 #FFF; }

  /* H5 */
  h5 { color: #333; font: italic normal bold 15px/15px Arial, Helvetica, Sans-Serif; text-shadow: 1px 1px 0 #FFF; }
 
  /* H6 */
  h6 { color: #333; font: italic normal normal 15px/15px Arial, Helvetica, Sans-Serif; text-shadow: 1px 1px 0 #FFF; }

  div.button { text-align: center;}
  div.banner {float:left; display:block;}
  div.formTips {
    color: #888888;
  }